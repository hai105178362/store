    <script nonce="$CSPNonce">
    
      function onAutoSignIn(googleUser) {
        // Useful data for your client-side scripts:
        var profile = googleUser.getBasicProfile();
        // The ID token you need to pass to your backend:
        var id_token = googleUser.getAuthResponse().id_token;
        console.log("ID Token: get successfully");
        if(jQuery(location).attr('hash') == '#autologin' && jQuery(location).attr('pathname').indexOf('identifydiscount.html') == -1){
        
        jQuery.ajax({
            type: 'POST',
            url: "ssoautologin.ajx",
            dataType: 'json',
            cache: false,
            data: "vid=20160512512&gtoken=" + id_token + "&discount_eligible=",
            success: function (data) {
                if(data.status){
                  if(data.target){
                    window.location=data.target;
                  }else{
                    updateLoginArea();
                    if(location.href.indexOf("basket.html") > -1){
                      window.location="basket.html?vid=20160512512";
                    }
                  }
                  
                }else{
                  alert("fail to auto login " + data.msg);
                }
                
            },
            error: function (data) {
                alert("auto login error "+data);
            }
        });
        
        }
        
      };
    </script>
    
