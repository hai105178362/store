            function changePageItemsCount(pageItemsCount){
                //pageItemsCount = jQuery(pageItemsCount).find(':selected').val();

                // TODO: change this to a switch...case

                if(pageItemsCount == '15'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&pageItemsCount=15';
                }

                if(pageItemsCount == '20'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&pageItemsCount=20';
                }

                if(pageItemsCount == '25'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&pageItemsCount=25';
                }
            }

            function sortRedirect(sortselect){
                // TODO: change this to a switch...case

                if(sortselect.value == 'stitleASC'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&sortci=stitle+asc';
                }

                if(sortselect.value == 'newestDESC'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&sortci=newest+desc';
                }

                if(sortselect.value == 'priceASC'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&sortci=price+asc';
                }

                if(sortselect.value == 'priceDESC'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&sortci=price+desc';
                }

                if(sortselect.value == 'topsellers'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&sortci=topsellers+asc';
                }

                if(sortselect.value == 'averagereview'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&sortci=averagereview+desc';
                }

                if(sortselect.value == 'orderscount'){
                    window.location.href = 'https://shop.googlemerchandisestore.com/store.html?cid=49562&vid=20160512512&tpt=theme1_en&sortci=orderscount+desc';
                }
            }

            jQuery(document).ready(function($){
				

                

                jQuery('.side-nav a.title .downArrow').hide();
                jQuery('input[type=checkbox].quicklook_trigger').removeAttr('disabled');
            });
        </script>

        
