        <script nonce="$CSPNonce">
            var urlParam = window.location.search.substring(1);
            var params = urlParam.split("&");
            var infScrollParams = "";
            
            for(i=0; i< params.length; i++) {
                if(params[i].substring(0, 3) != "vid" && params[i].substring(0, 3) != "tpt" && params[i].substring(0, 3) != "cid") {
                    infScrollParams = infScrollParams + "&" + params[i];
                }
                
            }
        </script>
        
