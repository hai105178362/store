    <script nonce="$CSPNonce">
    
    function signupNewsletter(id_suffix){
        //var email=jQuery("#newsletterSignup"+id_suffix).val();
        //if(email==''){alert("please input email!");return ;}
        jQuery.ajax({
            url : 'newslettersignup.ajx',
            //data : "email="+ email +"&vid="+ "20160512512"+"&randomKey="+new Date(),
            data : "vid="+ "20160512512"+"&randomKey="+new Date(),
            type : 'get',
            dataType: "json",
            success : function(data) {
                if('success'==data.__Message){
                    //alert("Subscribe to newsletter successfully!");
                    jQuery('#newsletterPopupBack').removeClass('hidden');
                    jQuery(".divNewsletter").hide();
                }else{
                    alert(data.__Message);
                }
            }
        });
    }
    
    //Minimum screen size for side menu to be fully expanded
    var minWidthSideMenuExpanded = 1600;

    // Initialize collapse button
    jQuery(".button-collapse").sideNav();
    jQuery("#slide-out").show();
    // Initialize collapsible (uncomment the line below if you use the dropdown variation)
    jQuery('.collapsible').collapsible();

    jQuery(document).ready(function() {
        jQuery('select').material_select();
        updateNavContainerClasses(minWidthSideMenuExpanded);
        
    });
    
    var delay;

    jQuery(window).scroll(function() {
    
        if (delay) { window.clearTimeout(delay); };
    
        delay = window.setTimeout(function () {
            
            if (jQuery(".navbar").offset().top > 50) {
                //jQuery('.navbar-row-1').hide('slow');
                //jQuery('.navbar-row-2 .right-container').show('slow');
                //jQuery('.navbar-row-2 .container-navbar-menu-button').show('slow');
                jQuery('body').addClass('scrolled');
            } else {
                //jQuery('.navbar-row-1').show('slow');
                //jQuery('.navbar-row-2 .right-container').hide('slow');
                //jQuery('.navbar-row-2 .container-navbar-menu-button').hide('slow');
                jQuery('body').removeClass('scrolled');
            }
            
        }, 100);
        
    });
    

    jQuery(window).resize(function() {
        updateNavContainerClasses(minWidthSideMenuExpanded);
    })

    //updateNavContainerClasses
    //swap container class for container-fluid when window width is less than "minWidthSideMenuExpanded"
    function updateNavContainerClasses(minWidthSideMenuExpanded) {
        if (window.innerWidth > minWidthSideMenuExpanded) {

        } else {

        }
    }
