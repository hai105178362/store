<script nonce="$CSPNonce">
    
    function GTMUserType() {
        
        var cname = "GTMType_20160512512=";
        var result = "";
        
        var cArray = document.cookie.split(';');
        
        for(var x = 0; x < cArray.length; x++) {
            var cTemp = cArray[x];
            while (cTemp.charAt(0)==' ') {
                cTemp = cTemp.substring(1);
            }
            if (cTemp.indexOf(cname) == 0) {
                result = cTemp.substring(cname.length,cTemp.length);
            }
        }
        
        if(result != "Recorded") {
        
                            var GTMType = "Retail";
                        
            dataLayer.push [{
                'userType': GTMType
            }];
            
            var dt = new Date();
            dt.setMinutes(dt.getMinutes() + 30);
            
            var expiry = "expires=" + dt.toUTCString();
            
            document.cookie = cname + "Recorded" + "; " + expiry;
            
        }

        
    }
    
</script>
    
    
