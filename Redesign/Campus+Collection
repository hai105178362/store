        <script type="text/javascript" nonce="$CSPNonce">
            function updateTopQtyBasketLines(elem){
		       update_top_basket();
			   var title = elem.currentSkuBO.item.domain.title; 
				showConfirmPopup("<strong>"+title+"</strong> was added to your shopping cart.", "Added to Cart", "Go to Shopping Cart");
		    }        

        	function showConfirmPopup(bodyText, titleText, buttonText) {
        		jQuery('#confirmBodyText').html(bodyText);
        		jQuery('#confirmPopupTitle').html(titleText);
        		jQuery('#confirmPopupBasketLink').html(buttonText);
        		jQuery("#confirmPopupBasketLink").attr("href", "basket.html?vid=20160512512");
        		jQuery('.confirmPopupBack').removeClass('hide');
        	}
        	
        	function closeConfirmPopup() {
        		jQuery('.confirmPopupBack').addClass('hide');
        	}
        </script>
