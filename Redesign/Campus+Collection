<script nonce="$CSPNonce">

function prodTagPush(action, iid, prodName, prodID, prodPrice, prodPos, prodCat, page, brand, quantity) {

    

    var pageType;
        
    if(page == "category") {
        pageType = "category";
    }
    else if(page == "search") {
        pageType = "search results";
    }

    if(action == "flip") {
    
        dataLayer.push({
            'event': 'view_item',
            'ecommerce': {
            'items': [{
                  'item_name': prodName,
                  'item_id': prodID,
                  'price': prodPrice,
                  'item_category': prodCat,
                  'G-JPEV636QX5' : pageType.toLowerCase(),
                  'item_brand' : brand,
                  'index': prodPos,
                  'name': prodName,
                  'id': prodID,
                  'category': prodCat,
                  'position': prodPos,
                  'brand' : brand
                }]
             }
        });
  
    }
    else if(action =="viewProd") {
    
    localStorage.setItem("gtm-20160512512-" + prodID, pageType);
    
        dataLayer.push({
            'event': 'productClick',
            'ecommerce': {
              'click': {
                'actionField': {'list': pageType},
                'products': [{
                  'name': prodName,
                  'id': prodID,
                  'price': prodPrice,
                  'category': prodCat,
                  'position': prodPos,
                  'name': prodName,
                  'id': prodID,
                  'category': prodCat,
                  'position': prodPos
                 }]
               }
             }
        });
    }
    else if(action == "addCart") {
    
    var variant = jQuery('#js-item-attributes-' + iid + " select option:selected").text();
    
    localStorage.setItem("gtm-20160512512-" + prodID, pageType);
    
    
        dataLayer.push({
          'event': 'add_to_cart',
              'ecommerce': {
                  'items': [{
                        'item_name': prodName,
                        'item_id': iid,
                        'price': prodPrice,
                        'currency' : 'USD',
                        'item_category': prodCat,
                        'item_brand' : brand,
                        'item_variant' : variant,
                        'G-JPEV636QX5' : pageType.toLowerCase(),
                        'quantity': quantity,
                        'name': prodName,
                        'id': prodID,
                        'category': prodCat,
                        'position': prodPos,
                        'brand' : brand,
                        'variant' : variant
                   }]
              }
        });
    }
  
}
</script>                    
                    
