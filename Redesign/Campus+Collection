function trackInteractionEvent (link, category, action, label) {
 try {
dataLayer.push({
    'event': category,
      'eventAction': action,
    'eventLabel': label
     });
        setTimeout('document.location = "' + link + '"', 100);
    } catch (err) {}
}
</script>        
        



                        
                        






                        








    <script>
    
        var validParams = true;
    
        var urlParams = new URLSearchParams(window.location.search);
    
        for(var key of urlParams.keys()) {
          if(key != 'tpt' && key != 'vid' && key != 'cid' && key != 'sortci' && key != 'Brand' && key != 'Subcategory' && key != 'Campus_Location' && key != 'Item_Type' && key != 'utm_content' && key != 'utm_campaign' && key != 'utm_medium' && key != 'utm_source') {
            validParams = false;
          }
        }
        
        if(!validParams) {
                                                window.location.href = "https://shop.googlemerchandisestore.com/";
                                    }
        
    </script>



            
